ate>\n  \n  <script>\n   import axios from 'axios'\n\n  export default {\n    data: () => ({\n        bpm: 1,\n      products: [],\n    }),\n     methods: {\n        decrement () {\n          this.bpm--\n        },\n        increment () {\n          this.bpm++\n        },\n        showProductDetails (id){\n          console.log(id);\n          window.location.href = \"/crabbe\";\n        }\n       \n      },\n    mounted () {\n    axios\n      .get('http://127.0.0.1:8402/products',{\n      headers: {\n        'Authorization': `Bearer ${this.$keycloak.token}`\n\n      }})\n      .then(response => (\n        this.products = response.data.products\n        ))\n  }\n  };\n  </script>\n  \n  <style>\n  </style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}