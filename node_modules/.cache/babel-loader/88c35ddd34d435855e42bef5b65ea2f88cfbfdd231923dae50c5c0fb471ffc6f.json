yment: \"Paypal\",\n        totalSum: \"145,90 EUR\",\n        details:[\n            {name: \"Crabbe Trabbe\", price: \"12,50 EUR\", amount: 1}\n        ]\n    }),\n    mounted(){\n        let orderId =  this.$route.params.id\n         axios\n      .get('http://localhost:8402/order/'+orderId,{\n      headers: {\n        'Authorization': `Bearer ${this.$keycloak.token}`\n\n      }})\n      .then(response => (\n        this.cartItems = response.data.cart.items,\n        this.totalSum = response.data.cart.totalAmount\n        ))\n    }\n}\n</script>\n<style>\n.pinkLink{\n    color: rgb(181, 27, 208) !important;\n}\n.width80{\n    width: 50%;\n}\n.paddingTop100{\n    padding-top: 100px !important;\n  }\n.font16{\n    font-size: 16px !important;\n  }\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}