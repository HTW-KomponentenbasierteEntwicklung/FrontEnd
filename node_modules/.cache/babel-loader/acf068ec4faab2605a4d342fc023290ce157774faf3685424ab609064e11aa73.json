{"ast":null,"code":"import NavBar from \"../components/NavBar.vue\";\nimport OrderManagement from \"../components/OrderManagement.vue\";\nimport Favorite from \"@/components/FavoriteList.vue\";\nimport Vue from \"vue\";\nexport default {\n  components: {\n    NavBar,\n    OrderManagement,\n    Favorite\n  },\n  data: () => ({\n    username: \"Mia\",\n    date: new Date(Date.now() - new Date().getTimezoneOffset() * 60000).toISOString().substr(0, 10),\n    menu: false,\n    show: \"accountData\",\n    anrede: [\"Herr\", \"Frau\"],\n    items: [{\n      title: 'Meine Bestellungen',\n      action: 'orderManagement'\n    }, {\n      title: 'Wunschliste',\n      action: \"favorites\"\n    }],\n    orders: [{\n      orderId: '#123',\n      date: \"19-12-2021\",\n      totalAmount: \"423.99 € für 2 Artikel\"\n    }],\n    products: [{\n      img: \"01.png\",\n      title: \"Cabbage\",\n      subtitle: \"1kg\",\n      price: \"$13\"\n    }, {\n      img: \"02.png\",\n      title: \"Perry's Ice Cream\",\n      subtitle: \"1kg\",\n      price: \"$23\"\n    }]\n  }),\n  methods: {\n    logout: function () {\n      Vue.$keycloak.logout();\n    }\n  },\n  mounted() {\n    axios.get('http://localhost:8402/order/username/' + this.$username, {\n      headers: {\n        'Authorization': `Bearer ${this.$keycloak.token}`\n      }\n    }).then(response => this.orders = response.data);\n  }\n};","map":{"version":3,"mappings":"AAgDA;AACA;AACA;AACA;AACA;EACAA;IACAC;IACAC;IACAC;EACA;EACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC,QACA;MAAAC;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,EACA;IACAC,SACA;MACAC;MACAR;MACAS;IACA,EAEA;IACAC,WACA;MAAAC;MAAAN;MAAAO;MAAAC;IAAA,GACA;MAAAF;MAAAN;MAAAO;MAAAC;IAAA;EAEA;EACAC;IACAC;MACAC;IACA;EACA;EACAC;IACAC,MACAC;MACAC;QACA;MAEA;IAAA,GACAC,iBACA,2BACA;EACA;AACA","names":["components","NavBar","OrderManagement","Favorite","data","username","date","menu","show","anrede","items","title","action","orders","orderId","totalAmount","products","img","subtitle","price","methods","logout","Vue","mounted","axios","get","headers","then"],"sourceRoot":"src/views","sources":["AccountManagementView.vue"],"sourcesContent":["<template>\n<v-app>\n      <NavBar />\n      <v-container fluid class=\"paddingTop100 alignleft px-15\">\n        <v-row>\n          <v-col cols=\"3\">\n            <v-card class=\"rounded-xl shadowCenter\">\n          <v-img src=\"cover.png\" max-height=\"50%\" max-width=\"100%\" contain>\n            <v-card-title class=\" mt-5 ml-5\">\n              <h1 align=\"left\" >Hallo {{username}}</h1>\n            </v-card-title>\n          </v-img>\n          \n        <v-list id=\"menuList\" class=\"mx-5\">\n            <v-list-item\n            v-for=\"(item, i) in items\" :key=\"i\" @click=\"show = item.action\"\n          ><v-hover v-slot=\"{ hover }\">\n            <v-list-item-title :key=\"i\" :class=\"{ 'bold-hover': hover }\" class=\"mx-5\">\n              {{item.title}}\n              </v-list-item-title>\n              \n          </v-hover>\n       \n          </v-list-item>\n          \n        </v-list>\n        <div align=\"center\" class=\"mx-10 py-5\">\n          <v-btn class=\"rounded-xl shadowCenter fullGrowX\" @click=\"logout()\"><h3>ABMELDEN</h3></v-btn>\n        </div>\n        \n        </v-card>\n          </v-col>\n        <v-col cols=\"9\">\n          <div class=\"ml-5\" v-if=\"show === 'orderManagement'\">\n            <OrderManagement :orders=\"orders\"/>\n          </div>\n          <div class=\"ml-5\" v-if=\"show === 'favorites'\">\n            <Favorite :products=\"products\"/>\n          </div>\n        </v-col>\n\n        </v-row>\n        \n        \n</v-container>\n</v-app>\n</template>\n<script>\n  import NavBar from \"../components/NavBar.vue\";\n  import OrderManagement from \"../components/OrderManagement.vue\";\n  import Favorite from \"@/components/FavoriteList.vue\";\n  import Vue from \"vue\";\n export default {\n  components: {\n      NavBar,\n      OrderManagement,\n      Favorite,\n  },\n    data: () => ({\n        username: \"Mia\",\n        date: (new Date(Date.now() - (new Date()).getTimezoneOffset() * 60000)).toISOString().substr(0, 10),\n        menu: false,\n        show: \"accountData\",\n        anrede: [\"Herr\", \"Frau\"],\n        items: [\n      { title: 'Meine Bestellungen', action: 'orderManagement' },\n      { title: 'Wunschliste', action: \"favorites\" },\n    ],\n        orders: [\n              {\n                orderId: '#123',\n                date: \"19-12-2021\",\n                totalAmount: \"423.99 € für 2 Artikel\",\n              }\n              \n            ],\n        products: [\n        { img: \"01.png\", title: \"Cabbage\", subtitle: \"1kg\", price: \"$13\" },\n        { img: \"02.png\", title: \"Perry's Ice Cream\", subtitle: \"1kg\", price: \"$23\" },\n        ]\n  }),\n    methods: {\n      logout: function(){\n        Vue.$keycloak.logout();\n      }\n    },\n  mounted() {\n    axios\n      .get('http://localhost:8402/order/username/'+this.$username,{\n      headers: {\n        'Authorization': `Bearer ${this.$keycloak.token}`\n\n      }})\n      .then(response => (\n        this.orders = response.data\n        ))\n  }\n  }\n  </script>\n  <style>\n  body {\n    font-size: 16px ;\n    font-weight: 400 ;\n}\n\n.paddingTop100{\n    padding-top: 100px !important;\n  }\n  .alignleft {\n    text-align: left !important;\n  }\n\n  .whiteFont{\n    color: white;\n  }\n  .v-list-item{\n    border-bottom: .0625rem solid #ebebeb;\n  }\n\n  .v-list-item__title.bold-hover{\n    font-weight: bold;\n  }\n  .fullGrowX{\n    width: 100%;\n    background-color: white !important;\n    \n  }\n  .shadowCenter{\n    box-shadow: 0 2px 8px 0 rgb(88 88 88 / 32%)\n  }\n  @media screen and (min-width: 375px){\n    .variableFontSize {\n    font-size: calc(24px + 24*(100vw - 375px)/ 1545);\n  }\n}\n.v-list-item {\n  border-bottom: 0;\n}\n  </style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}